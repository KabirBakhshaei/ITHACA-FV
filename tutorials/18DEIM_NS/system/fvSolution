FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        tolerance       1e-06;
        relTol          0;
        smoother        GaussSeidel;
    }

    U
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        nSweeps         2;
        tolerance       1e-08;
        relTol          0;
    }
    Phi
    {
        $p;
    }
}

relaxationFactors
{
    fields
    {
        p               0.3;
        Paux            0.3;
    }
    equations
    {
        U               0.7;
        nuTilda         0.7;
        Uaux            0.7;
    }
}

cache
{
    grad(U);
}

SIMPLE
{
    nCorrectors 10;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
    PhiRefCell 0;
    PhiRefPoint 0;
    PauxRefCell 0;
    PauxRefPoint 0;
    PauxRefValue 0;
}

potentialFlow
{
    nNonOrthogonalCorrectors 10;
