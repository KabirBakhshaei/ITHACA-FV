FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss linearUpwind grad(U);
    div(phi,subsetU)      Gauss linearUpwind grad(subsetU);
    div(interpolate(U),subsetU)      Gauss linearUpwind grad(subsetU);
    div(interpolate(Urec),U)      Gauss linearUpwind grad(Urec);
    div(interpolate(U),U)      Gauss linearUpwind grad(U);
}

laplacianSchemes
{
    default         none;
    laplacian(nu,U) Gauss linear corrected;
    laplacian(nu,subsetU) Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


// ************************************************************************* //
